#axiom
^gv[+A]-f>G

#rules
A: FrA

#instructions
F: DRAW, 0.03
G: DRAW, 3.5
f: MOVE, 3
g: MOVE, 0.1
[: PUSH, 0
]: POP, 0
v: PITCH, 90
^: PITCH, -90
+: YAW, 90
-: YAW, -90
>: ROLL, -90
r: PITCH, -1

#settings
generations: 360
lineThickness: 0.1
lineThicknessModifier: 1
lineLengthModifier: 1
pruneChance: 0
mutationChance: 0
